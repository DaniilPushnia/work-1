#include <stdio.h>

int big(int a, int b) {
    while (b != 0) {
        int t = b;
        b = a % b;
        a = t;
    }
    return a;
}

int small(int a, int b) {
    return (a * b) / big(a, b);
}

int main() {
    int p;
    printf("Введіть кількість чисел: ");
    scanf("%d", &p);

    int numbers[p];
    printf("Введіть числа: ");
    for (int i = 0; i < p; i++) {
        scanf("%d", &numbers[i]);
    }

    int result = numbers[0];
    for (int i = 1; i < p; i++) {
        result = small(result, numbers[i]);
    }

    printf("Найменше спільне кратне: %d\n", result);

    return 0;
}
